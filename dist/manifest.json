{
  "manifest_version": 3,
  "name": "GRE Vocabulary Highlighter",
  "description": "Highlight GRE vocabulary and phrases on every web page you visit.",
  "author": "jimmyhuang29@g.ucla.edu",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "scripting"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.index.jsx.6a8d6a95.70256690.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "assets/index.jsx.ba119cc2.css"
      ]
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      }
    }
  },
  "icons": {
    "16": "assets/icons/logo16.png",
    "32": "assets/icons/logo32.png",
    "48": "assets/icons/logo.png",
    "128": "assets/icons/logo.png"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/jsx-runtime.ce3a7f6b.js",
        "assets/message.492f0669.js",
        "assets/index.jsx.6a8d6a95.js"
      ],
      "use_dynamic_url": true
    }
  ]
}